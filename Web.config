<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpPlatformHandlerMain" modules="httpPlatformHandler" path="*" verb="*" resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="D:\home\site\wwwroot\startup.bat" arguments="" startupTimeLimit="60" startupRetryCount="5">
      <environmentVariables>
        <environmentVariable name="RUBY_VERSION" value="2.0.0" />
        <environmentVariable name="RUBY_ENV" value="production" />
        <environmentVariable name="RAILS_ROOT" value="D:\home\site\wwwroot" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
</configuration>
